.close_an_account_container {
    width: 100%;
    height: 88rpx;
    display: flex;
    flex-direction: row;
    position: fixed;
    background-color: #ffffff;
    bottom: 0;
    align-items: center;
}
.left_container {
    width: 70%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.all_select_container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    margin-left: 30rpx;
}
.all_select {
    margin-left: 25rpx;
    font-size: 24rpx;
    color: #333333;
}
.all_money_container {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    margin-right: 20rpx;
}
.all_money {
    font-size: 26rpx;
    color: #333333;
    margin-right: 25rpx;
}
.count_container {
    width: 30%;
    height: 100%;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    background-color: #157658;
}
.count {
    font-size: 28rpx;
    color: #ffffff;

}